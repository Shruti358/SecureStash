{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "web/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "web/$1" }
  ],
  "headers": [
    {
      "source": "/downloads/(.*)\\.apk",
      "headers": [
        { "key": "Content-Type", "value": "application/vnd.android.package-archive" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ]
}
